{
 "description": "A subuser with all permissions",
 "maintainer": "Timothy Hobbs <timothyhobbs@seznam.cz>",
 "executable": "/bin/bash",
 "entrypoints": {"foo":"/usr/bin/foo"},
 "basic-common-permissions": {"stateful-home":true,"inherit-timezone":false},
 "memory-limit": "3g",
 "max-cpus": 3.5,
 "gui": {"clipboard":true,"border-color":"yellow"},
 "user-dirs": ["Downloads"],
 "inherit-envvars": ["GCC"],
 "sound-card": true,
 "webcam": true,
 "access-working-directory": true,
 "allow-network-access": true,
 "x11": false,
 "system-dirs":{"/var/log":"/host/logs"},
 "graphics-card": true,
 "serial-devices": true,
 "system-dbus": true,
 "sudo": true,
 "as-root": true,
 "privileged": true,
 "run-commands-on-host": true
}
